<service>
	<id>mov1lab.exe</id>
	<name>mov1lab</name>
	<description>Monitor for the process that sends messages from cobas.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Program Files (x86)\Mov1Lab\mov1lab\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\Aplicaciones\mov1lab\source\mov1lab\app.js</argument>
	<argument>--log</argument>
	<argument>mov1lab wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>C:\Program Files (x86)\Mov1Lab\mov1lab</workingdirectory>
</service>